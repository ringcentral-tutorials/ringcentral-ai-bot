    "s:watch": "cd lamda && ../node_modules/.bin/serverless logs -f hello -t",
    "lint": "./node_modules/.bin/eslint --max-warnings 0 --ext .js  src",
    "start": "npm run dev",
    "dev": "node ./src/server/run-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:zxdong262/rc-ai-bot.git"
